@model IEnumerable<Order>
@{
    ViewBag.Title = "Общая ведомость";
}

<section class="breadcrumbs-custom bg-image" style="background-image: url(../images/breadcrumbs-bg.png);"
         data-preset="{&quot;title&quot;:&quot;Breadcrumbs&quot;,&quot;category&quot;:&quot;header&quot;,&quot;reload&quot;:false,&quot;id&quot;:&quot;breadcrumbs&quot;}">
    <div class="container">
        <div class="row row-20 align-items-center">
            <div class="col-sm-6">
                <h5 class="breadcrumbs-custom-title">Общая ведомость</h5>
            </div>
        </div>
    </div>
</section>


@await Html.PartialAsync("_Status.cshtml")

@foreach (var item in Model)
{
    @await Html.PartialAsync("_CurrentOrder.cshtml", item)
}


